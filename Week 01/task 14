//task14
int main() 
{
    int num1, num2, num3, num4, num5;
    int sum = 0;
    std::cin >> num1 >> num2 >> num3 >> num4 >> num5;

    sum += (num1 % 2 != 0) * num1;
    sum += (num2 % 2 != 0) * num2;
    sum += (num3 % 2 != 0) * num3;
    sum += (num4 % 2 != 0) * num4;
    sum += (num5 % 2 != 0) * num5;

    std::cout << sum << std::endl;
    return 0;
}
